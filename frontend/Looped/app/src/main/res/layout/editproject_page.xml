<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/editproject_page"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FDF4E6"
    tools:context=".Pages.EditProjectPage">

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="410dp"
        android:layout_height="162dp"
        android:src="@drawable/profile_block"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <EditText
        android:id="@+id/editFaveColors"
        android:layout_width="338dp"
        android:layout_height="48dp"
        android:autofillHints=""
        android:background="@drawable/stroke_rounded_corner"
        android:ems="10"
        android:fontFamily="@font/inter_medium"
        android:hint="@string/fave_colors"
        android:inputType="textEmailAddress"
        android:paddingStart="20dp"
        android:textColor="@color/dark_purple"
        android:textColorHint="@color/dark_purple"
        android:textSize="15sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.493"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView6"
        tools:ignore="RtlSymmetry" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="224dp"
        android:fontFamily="@font/inter_medium"
        android:text="Username"
        android:textColor="@color/grey"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.096"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:fontFamily="@font/inter_medium"
        android:text="About me"
        android:textColor="@color/grey"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.092"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextLocation" />

    <EditText
        android:id="@+id/editTextFirstName"
        android:layout_width="338dp"
        android:layout_height="48dp"
        android:autofillHints=""
        android:background="@drawable/stroke_rounded_corner"
        android:ems="10"
        android:fontFamily="@font/inter_medium"
        android:hint="@string/first_name"
        android:inputType="textEmailAddress"
        android:paddingStart="20dp"
        android:textColor="@color/dark_purple"
        android:textColorHint="@color/dark_purple"
        android:textSize="15sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.452"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3"
        tools:ignore="RtlSymmetry" />

    <EditText
        android:id="@+id/editTextLocation"
        android:layout_width="338dp"
        android:layout_height="48dp"
        android:layout_marginTop="3dp"
        android:autofillHints=""
        android:background="@drawable/stroke_rounded_corner"
        android:ems="10"
        android:fontFamily="@font/inter_medium"
        android:hint="@string/location"
        android:inputType="textEmailAddress"
        android:paddingStart="20dp"
        android:textColor="@color/dark_purple"
        android:textColorHint="@color/dark_purple"
        android:textSize="15sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.452"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4"
        tools:ignore="RtlSymmetry" />

    <EditText
        android:id="@+id/editTextUsername"
        android:layout_width="338dp"
        android:layout_height="48dp"
        android:layout_marginTop="4dp"
        android:autofillHints=""
        android:background="@drawable/stroke_rounded_corner"
        android:ems="10"
        android:fontFamily="@font/inter_medium"
        android:hint="bojack234"
        android:inputType="textEmailAddress"
        android:paddingStart="20dp"
        android:textColor="@color/dark_purple"
        android:textColorHint="@color/dark_purple"
        android:textSize="15sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.479"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2"
        tools:ignore="RtlSymmetry" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/inter_medium"
        android:text="Location"
        android:textColor="@color/grey"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.093"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextFirstName" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/inter_medium"
        android:text="First name"
        android:textColor="@color/grey"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.105"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextUsername" />

    <EditText
        android:id="@+id/editTextAboutMe"
        android:layout_width="338dp"
        android:layout_height="48dp"
        android:autofillHints=""
        android:background="@drawable/stroke_rounded_corner"
        android:ems="10"
        android:fontFamily="@font/inter_medium"
        android:hint="@string/about_me"
        android:inputType="textEmailAddress"
        android:paddingStart="20dp"
        android:textColor="@color/dark_purple"
        android:textColorHint="@color/dark_purple"
        android:textSize="15sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.452"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5"
        tools:ignore="RtlSymmetry" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:fontFamily="@font/inter_medium"
        android:text="Fave colors"
        android:textColor="@color/grey"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.098"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextAboutMe" />

</androidx.constraintlayout.widget.ConstraintLayout>